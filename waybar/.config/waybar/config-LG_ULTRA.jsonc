    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "  ÔÅÆ  ",
            "deactivated": "  ÔÅ∞  "
        }
    },
    "tray": {
        "spacing": 10,
		"show-passive-items": true
